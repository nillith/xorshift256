{
  "check-coverage": true,
  "per-file": true,
  "extensions": [
    ".ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
        "src/**/*.spec.ts"
    ],
  "ignore-class-method": "methodToIgnore",
  "temp-directory": "./coverage/.nyc_output",
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "cobertura",
    "html",
    "text",
    "text-summary"
  ]
}